{
  "expo": {
    "name": "Fairzy",
    "slug": "fairzy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/generated-image-1768414893142.png",
    "scheme": "fairzy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/generated-image-1768414893142.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fairzy.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/generated-image-1768414893142.png",
        "backgroundColor": "#000000"
      },
      "package": "com.fairzy.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/generated-image-1768414893142.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "fairzy-turn-tracker"
      }
    }
  }
}
